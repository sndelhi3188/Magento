/********** < Navigation */
.mw_navigation{background-color:#3a3b40;}
#nav { width:962px; left:0; position:relative;font-size:12px;padding:0px 0; margin:0 auto;z-index:10; }

/* All Levels */
#nav ul { padding-right:0; margin-left:7px!important; margin-left:14px; }
#nav li { float:left; text-align:left;position: relative; }
#nav li.over { z-index:999; }
#nav a{  float:left; font-size:18px; font-weight:normal; color:#999999; display:block; padding:7px 20px; margin-left:7px; line-height:normal;}
#nav span { display:block; cursor:pointer; white-space:nowrap; }
#nav li ul span { white-space:normal; }
#nav li li.parent {}

/* 0 Level */
#nav li { float:left; }
#nav li.active a { background:#00aeef; color:#fff;  }
#nav a { float:left; color:#999999; }
#nav li.over a,
#nav a:hover { position:relative; color:#fff; background:#00aeef; text-decoration:none; }

/* 1st Level */
#nav ul li,
#nav ul li.active { float:none; margin:0; padding-bottom:1px; background-color:#e0e0e0; }
#nav ul li.over {}
#nav ul li.last { background-color:#444 ; padding-bottom:0; }

#nav ul a,
#nav ul a:hover { line-height:normal; float:none; padding:0; background:none; }
#nav ul li a { font-weight:normal !important; font-size:12px; margin:0px; }

/* 2nd leven */
#nav ul { position:absolute; width:15em; top:35px; left:-10000px;z-index:10;/* border:1px solid #ddd;*/ }

/* 3rd+ Level */
#nav ul ul { top:5px; }

/* Show Menu */
#nav li.over > ul { left:0; }
#nav li.over > ul li.over > ul { left:150px; }
#nav li.over ul ul { left:-10000px; }

#nav ul li a { background-color:#e0e0e0; color:#43AAE4; }
#nav ul li a:hover { background-color:#3a3b40; color:#fff !important; }
#nav ul span,
#nav ul li.last li span { padding:6px 10px 5px 20px; }
/********** Navigation > */
