/* Global Styles ========================================================================= */
/* Form Elements */
input,select,textarea,button { font:12px Arial, Helvetica, sans-serif; vertical-align:middle; color:#707070; }
input.input-text,select,textarea { background-color:#fff; border:1px solid #ddd; }
